  <!-- Actuator definitions-->
  <actuator>
    <position class="suction_arm" name="shoulder" joint="shoulder_joint" forcerange="-87 87"/>
    <position class="suction_arm" name="deltoid" joint="deltoid_joint" forcerange="-87 87"/>
    <position class="suction_arm" name="forearm" joint="forearm_joint" forcerange="-87 87"/>
    <position class="suction_arm" name="wrist" joint="wrist_joint" forcerange="-87 87"/>

  </actuator>

  <contact>
    <exclude name="exclude_base_shoulder" body1="base_link" body2="shoulder_link"/>
    <exclude name="exclude_base_deltoid" body1="shoulder_link" body2="deltoid_link"/>
    
  </contact>


  <keyframe>
